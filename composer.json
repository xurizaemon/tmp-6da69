{
    "name": "xurizaemon/demo-renovate",
    "description": "A Drupal + Renovate demo",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^2.3",
        "cweagans/composer-patches": "^1.7.3",
        "dlcs/iiif-php": "^1.2",
        "drupal/admin_toolbar": "^3.5.3",
        "drupal/allowed_formats": "^3",
        "drupal/book": "^2.0",
        "drupal/bootstrap_barrio": "^5.5.19",
        "drupal/bootstrap_sass": "^5.0",
        "drupal/ckeditor_html_embed": "^1.0",
        "drupal/config_filter": "^2.7.0",
        "drupal/config_split": "^2",
        "drupal/config_update": "^2.0.0-alpha4@alpha",
        "drupal/content_browser": "^1.0",
        "drupal/controlled_access_terms": "^2.0",
        "drupal/core-composer-scaffold": "~10.4.7",
        "drupal/core-recommended": "~10.4.7",
        "drupal/csp": "^2.2.2",
        "drupal/csv_serialization": "^4",
        "drupal/devel": "^5.1",
        "drupal/diff": "^2.0.0-beta3@beta",
        "drupal/dis": "^1.0.2",
        "drupal/editor_advanced_link": "^2.2",
        "drupal/entity_browser": "^2.3",
        "drupal/entity_clone": "^2.0@beta",
        "drupal/entity_embed": "^1.6",
        "drupal/entity_pager": "^1.0.0",
        "drupal/entity_reference_purger": "^1.0@beta",
        "drupal/entity_reference_unpublished": "^2.0",
        "drupal/entityqueue": "^1.9.0",
        "drupal/environment_indicator": "^4.0.21",
        "drupal/facets": "^2.0",
        "drupal/features": "^3.14",
        "drupal/field_group": "^3.4",
        "drupal/field_permissions": "^1.0",
        "drupal/file_access_fix": "^1.0.0",
        "drupal/filehash": "^3.0.5",
        "drupal/fixed_block_content": "^1.3",
        "drupal/flysystem": "^2.2@alpha",
        "drupal/form_options_attributes": "^2.0",
        "drupal/format_bytes": "^2.0.3",
        "drupal/geolocation": "^3.2",
        "drupal/gin": "^4.0.2",
        "drupal/gin_toolbar": "^2.0.0",
        "drupal/google_analytics": "^4.0.3",
        "drupal/islandora": "^2.16.0",
        "drupal/jsonlog": "^4.0",
        "drupal/key": "^1.11",
        "drupal/libraries": "^4.0",
        "drupal/login_emailusername": "^2.1",
        "drupal/login_security": "^2",
        "drupal/masquerade": "^2.0@beta",
        "drupal/media_parent_entity_link": "^1.3.0",
        "drupal/memcache": "^2.5",
        "drupal/metatag": "^2.0",
        "drupal/migrate_devel": "^3.0",
        "drupal/migrate_source_ui": "^1.0",
        "drupal/migrate_tools": "^6.0",
        "drupal/modules_weight": "^3.0.2",
        "drupal/name": "^1.0.0",
        "drupal/openseadragon": "^2.2",
        "drupal/password_policy": "^4.0.3",
        "drupal/pathauto": "^1.13.0",
        "drupal/pdf": "^1.2.0",
        "drupal/queue_ui": "^3.2.1",
        "drupal/raven": "^6.0",
        "drupal/rdf": "^2.0",
        "drupal/readmehelp": "^2.0@beta",
        "drupal/recaptcha": "^3.4.0",
        "drupal/redirect": "^1.7",
        "drupal/request_info": "^1.5.0",
        "drupal/rest_oai_pmh": "^2.0.2",
        "drupal/restui": "^1.22.0",
        "drupal/rules": "^4.0.0",
        "drupal/search_api": "^1.21",
        "drupal/search_api_solr": "^4.3.10",
        "drupal/simple_sitemap": "^4.2.2",
        "drupal/slick": "^3.0",
        "drupal/slick_views": "^3.0",
        "drupal/smart_trim": "^2.0",
        "drupal/structure_sync": "^2.0",
        "drupal/sync_uuids": "^1.1",
        "drupal/token": "^1.15.0",
        "drupal/tr_rulez": "^2.0.0",
        "drupal/trim": "^2.0.1",
        "drupal/twig_tweak": "^3.2",
        "drupal/typed_data": "^2.1.1",
        "drupal/views_bulk_edit": "^3.0",
        "drupal/views_bulk_operations": "^4.3.4",
        "drupal/views_data_export": "1.4",
        "drupal/views_order_by_delta": "3.x-dev@dev",
        "drush/drush": "^12",
        "previousnext/nested-set": "^2.0"
    },
    "require-dev": {
        "behat/behat": "^3.21.1",
        "dmore/behat-chrome-extension": "^1.4.0",
        "drevops/behat-screenshot": "^1.2.0",
        "drevops/behat-steps": "^2.5.0",
        "drupal/core-dev": "~10.4.7",
        "drupal/drupal-extension": "^5.1.0",
        "drupal/stage_file_proxy": "^3.1.5@alpha",
        "drupal/upgrade_status": "^4.0",
        "enlightn/security-checker": "^1.9",
        "kint-php/kint": "^5.1",
        "palantirnet/drupal-rector": "^0.20",
        "phpro/grumphp-shim": "^2.0",
        "phpspec/prophecy-phpunit": "^2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "php-http/discovery": true,
            "phpro/grumphp-shim": true,
            "phpstan/extension-installer": true,
            "symfony/runtime": true,
            "tbachert/spi": false
        },
        "platform": {
            "php": "8.1.28"
        }
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "allowed-packages": [
                "drupal/core"
            ],
            "file-mapping": {
                "[web-root]/sites/development.services.yml": false,
                ".editorconfig": false,
                ".gitattributes": false
            }
        }
    }
}
