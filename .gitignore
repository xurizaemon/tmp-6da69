# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore sensitive information
/web/sites/*/settings.local.php

# Ignore configuration files that may contain sensitive information.
/web/sites/*/services*.yml

# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore deployment identifier, this should be generated automatically.
/deployment_identifier.txt

# Ignore phpunit artifacts
.phpunit.result.cache

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by PhpStorm
/.idea/

# Ignore .env files as they are personal
/.env

# Ignore files generated by NPM
node_modules/

# Ignore test screenshots
/tests/screenshots
/tests/behat/screenshots

# Ignore vscode developer customisations
/.vscode
/.phpactor.json

# Ignore database dumps
*.sql*

# Ignore app data (backups, exports and logs)
/data/

# Ignore Lando local overrides
.lando.local.yml

# Ignore local phpstan configuration
/phpstan.neon

# Ignore firecow/gitlab-ci-local
.gitlab-ci-local-variables.yml
.gitlab-ci-local/

# Ignore dotenv
.env
